{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/category/organizations",
      "destination": "/setup"
    },
    {
      "source": "/organizations",
      "destination": "/setup"
    },
    {
      "source": "/docs/:path*",
      "destination": "/:path*"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "build",
        "buildCommand": "npm run vercel-build"
      }
    }
  ]
}
